    <validationRules>
        <fullName>Address_Mandatory</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
OR(
ISBLANK(Country),
ISBLANK(Street),
ISBLANK(City),
AND(NOT(OR((Country = &apos;Caribbean&apos;),
(Country = &apos;Singapore&apos;),
(Country = &apos;Korea, Republic of&apos;))),
ISBLANK(State)),
ISBLANK(PostalCode)
),
NOT(
OR(
$Profile.Name = &apos;CSS Integration&apos;,
$Profile.Name = &apos;System Administrator&apos;,
$Profile.Name = &apos;CSS_Support&apos;
)
)
)</errorConditionFormula>
        <errorMessage>Please make sure you have entered Street, City, State, Country, and Zip/Postal Code in Address.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>FSL_Address_New_Line_Validate</fullName>
        <active>true</active>
        <errorConditionFormula>REGEX( Street , &apos;(.*\r?\n.*)*&apos;)</errorConditionFormula>
        <errorDisplayField>Street</errorDisplayField>
        <errorMessage>Address lines must be continuous and separated by a comma.
Address field must be in the following format: Address Line 1, Line 2, Line 3</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>FSL_BillingPostalCode_validate_Can</fullName>
        <active>true</active>
        <description>As part of story FBI-44, to add validation for postal codes.</description>
        <errorConditionFormula>AND(
        OR(Country = &quot;CAN&quot;, Country = &quot;CA&quot;, Country = &quot;Canada&quot;),
        NOT(REGEX(PostalCode, &quot;((?i)[ABCEGHJKLMNPRSTVXY]\\d[A-Z]?\\s?\\d[A-Z]\\d)?&quot;)),
		NOT(ISBLANK(  Street  )), 
$Profile.Name &lt;&gt; &apos;System Administrator&apos;, 
$Profile.Name &lt;&gt; &apos;CSS Integration&apos;, 
$Profile.Name &lt;&gt; &apos;CSS_Support&apos;,
OR(ISNEW(), ISCHANGED(PostalCode) )
)</errorConditionFormula>
        <errorDisplayField>PostalCode</errorDisplayField>
        <errorMessage>Canadian postal code must be in A9A 9A9 format.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>FSL_BillingPostalCode_validate_US</fullName>
        <active>true</active>
        <description>As part of story FBI-44, to add validation for postal codes.</description>
        <errorConditionFormula>AND(
OR(Country = &quot;USA&quot;, Country = &quot;US&quot;,Country = &quot;United States&quot;),
NOT(REGEX(PostalCode, &quot;\\d{5}(-\\d{4})?&quot;)), 
NOT(ISBLANK(  Street  )), 
$Profile.Name &lt;&gt; &apos;System Administrator&apos;, 
$Profile.Name &lt;&gt; &apos;CSS Integration&apos;, 
$Profile.Name &lt;&gt; &apos;CSS_Support&apos;,
OR(ISNEW(), ISCHANGED(PostalCode) )
)</errorConditionFormula>
        <errorDisplayField>PostalCode</errorDisplayField>
        <errorMessage>Zipcode must be in 99999 or 99999-9999 format.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>FSL_Tax_District_Canada</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
  Country  == &apos;Canada&apos;,
  NOT(ISBLANK(TEXT(FSL_Tax_District__c))),
  OR(
    ISPICKVAL(FSL_Tax_District__c, &quot;----- Canada&quot;),
    ISPICKVAL(FSL_Tax_District__c, &quot;----- Caribbean&quot;),
    ISPICKVAL(FSL_Tax_District__c, &quot;CATANO&quot;),
    ISPICKVAL(FSL_Tax_District__c, &quot;CATANOL&quot;),
    ISPICKVAL(FSL_Tax_District__c, &quot;CATANOX&quot;),
