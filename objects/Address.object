    )
  )
)</errorConditionFormula>
        <errorDisplayField>FSL_Tax_District__c</errorDisplayField>
        <errorMessage>Please choose a Tax District under ----- Canada.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>FSL_Tax_District_Caribbean</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
  Country  == &apos;Caribbean&apos;,
