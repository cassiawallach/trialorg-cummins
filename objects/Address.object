$Profile.Name = &apos;CSS_Support&apos;
)
)
)</errorConditionFormula>
        <errorDisplayField>FSL_Tax_District__c</errorDisplayField>
        <errorMessage>Please make sure to choose a Tax District.</errorMessage>
    </validationRules>
    <validationRules>
