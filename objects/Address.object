        <errorDisplayField>FSL_Tax_District__c</errorDisplayField>
        <errorMessage>Please choose a Tax District under ----- Caribbean.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Tax_District_Mandatory</fullName>
        <active>true</active>
        <description>Tax District Mandatory for Countries ‘Canada&apos; and &apos;Caribbean’.</description>
        <errorConditionFormula>AND(OR(Country == &apos;Canada&apos;, Country == &apos;Caribbean&apos;), ISBLANK(TEXT(FSL_Tax_District__c)),
NOT(
