        <deprecated>false</deprecated>
        <externalId>false</externalId>
        <formula>If ( disco__Active__c  &lt;&gt;  true,&apos;Inactive&apos;, 
    IF (  NOT(ISPICKVAL(disco__Form_Template__r.disco__Status__c,&apos;In Use&apos;)),&apos;Template not published&apos;,
		IF(disco__Start_Date__c &gt; TODAY(),&apos;Future&apos;,
	IF(disco__End_Date__c &lt; TODAY() || disco__Start_Date__c  = NULL || ISPICKVAL(disco__Frequency__c,&apos;Once&apos;) ,&apos;Past&apos;,
				IF(disco__End_Date__c &gt; TODAY() &amp;&amp; disco__Start_Date__c &lt;= TODAY(), &apos;Current&apos;, null)
			)
		)
	)
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
