    </searchLayouts>
    <sharingModel>ControlledByParent</sharingModel>
    <validationRules>
        <fullName>FSL_Validate_User_Profile</fullName>
        <active>true</active>
        <errorConditionFormula>NOT(OR(
FSL__User__r.Profile.Name = &quot;CSS_Service_Advanced&quot;,
FSL__User__r.Profile.Name = &quot;CSS Read Only&quot;,FSL__User__r.Profile.Name = &quot;System Administrator&quot;,
FSL__User__r.Profile.Name = &quot;CMI_Read_Only&quot;,
FSL__User__r.Profile.Name = &quot;CMI System Admin&quot;,
FSL__User__r.Profile.Name = &quot;CSS_Support&quot;

))</errorConditionFormula>
        <errorMessage>Only Advanced users can be associated to User Territories as Users</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>FSL_Validate_User_Setup_Resource</fullName>
        <active>true</active>
        <errorConditionFormula>FSL__User__c &lt;&gt; FSL_Setup_Resource__r.User__c &amp;&amp; NOT (ISBLANK( FSL_Setup_Resource__c))</errorConditionFormula>
        <errorMessage>The User from the Setup Resource has to match the User of User Territory</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Parent_territories</fullName>
        <active>true</active>
        <description>CHF-48: As part of User territory creation we need to display only Parent Service territory dropdown however today we are showing both parent and child service territories</description>
        <errorConditionFormula>NOT(ISPICKVAL(FSL__ServiceTerritory__r.Type__c, &quot;Group&quot;))</errorConditionFormula>
        <errorDisplayField>FSL__ServiceTerritory__c</errorDisplayField>
        <errorMessage>Please select the parent territory</errorMessage>
    </validationRules>
    <visibility>Public</visibility>
</CustomObject>
