    <label>User Territory</label>
    <listViews>
        <fullName>FSL__All_User_Territories</fullName>
        <columns>NAME</columns>
        <columns>FSL__User__c</columns>
        <columns>FSL__ServiceTerritory__c</columns>
        <columns>FSL__Territory_Name__c</columns>
        <columns>LAST_UPDATE</columns>
        <columns>UPDATEDBY_USER</columns>
        <filterScope>Everything</filterScope>
        <filters>
            <field>FSL__User__c</field>
            <operation>contains</operation>
            <value>OAG</value>
        </filters>
        <label>All User Territories</label>
    </listViews>
    <nameField>
        <displayFormat>ULID-{000000}</displayFormat>
        <label>User Territory Number</label>
        <type>AutoNumber</type>
    </nameField>
    <pluralLabel>User Territories</pluralLabel>
