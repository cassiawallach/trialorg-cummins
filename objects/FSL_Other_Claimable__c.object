    <validationRules>
        <fullName>Description_Mandatory</fullName>
        <active>true</active>
        <description>Description mandatory when type is UNLISTED</description>
        <errorConditionFormula>AND( ISPICKVAL( Item__c , &apos;UNLISTED&apos;) , ISBLANK( Description__c ) )</errorConditionFormula>
        <errorDisplayField>Description__c</errorDisplayField>
        <errorMessage>Please provide a description for the Unlisted Other Claimable item.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>FSL_Service_Order_Closed_OtherClaimables</fullName>
        <active>true</active>
        <errorConditionFormula>OR( ISPICKVAL(Service_Order__r.Status, &quot;Closed&quot;),
ISPICKVAL(Service_Order__r.Status, &quot;Canceled&quot;))</errorConditionFormula>
        <errorMessage>Changes cannot be made after a job is closed</errorMessage>
    </validationRules>
