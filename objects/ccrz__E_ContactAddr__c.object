    <fields>
        <fullName>dbu_Formatted_AddressSecondline__c</fullName>
        <externalId>false</externalId>
        <formula>IF(
NOT(ISBLANK(ccrz__AddressFirstline__c)),
ccrz__AddressFirstline__c+ &apos; &apos; ,
NULL
) +
IF(
NOT(ISBLANK(ccrz__AddressSecondline__c)),
ccrz__AddressSecondline__c+ &apos; &apos; ,
NULL
)+
IF(
NOT(ISBLANK(ccrz__City__c)),
ccrz__City__c+ &apos; &apos;,
NULL
)+
IF(
NOT(ISBLANK(ccrz__State__c)),
ccrz__State__c+ &apos;, &apos;,
NULL
)+
IF(
NOT(ISBLANK(ccrz__PostalCode__c)),
ccrz__PostalCode__c+ &apos; &apos; ,
NULL
)+
IF(NOT(ISBLANK(ccrz__Country__c)),
IF(ccrz__Country__c == &apos;US&apos;,
&apos;U.S.A.&apos;,
IF(ccrz__Country__c == &apos;CA&apos;,&apos;CA&apos;, NULL)
)+ &apos; &apos;,NULL
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>dbu_Formatted_AddressSecondline</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>dbu_Formatted_Address__c</fullName>
        <externalId>false</externalId>
        <formula>IF(ccrz__AddressSecondline__c != &apos;&apos;,ccrz__AddressFirstline__c+&apos; &apos;+
ccrz__AddressSecondline__c+&apos; &apos;+
ccrz__City__c+&apos;, &apos;+ ccrz__State__c+&apos; &apos;+ ccrz__PostalCode__c+&apos; &apos;+
IF(ccrz__Country__c == &apos;US&apos;,&apos;U.S.A&apos;,ccrz__Country__c),
ccrz__AddressFirstline__c+&apos; &apos;+
ccrz__City__c+&apos;, &apos;+ ccrz__State__c+&apos; &apos;+ ccrz__PostalCode__c+&apos; &apos;+

IF(ccrz__Country__c == &apos;US&apos;,&apos;U.S.A&apos;,ccrz__Country__c))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Formatted Address</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>dbu_Formatted_Country__c</fullName>
        <externalId>false</externalId>
        <formula>IF( ccrz__Country__c == &apos;US&apos;,&apos;U.S.A.&apos;, &apos;Canada&apos; )</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
