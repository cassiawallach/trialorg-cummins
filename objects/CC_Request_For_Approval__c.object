    <webLinks>
        <fullName>Reject</fullName>
        <availability>online</availability>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Reject</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/37.0/connection.js&quot;)} 
{!REQUIRESCRIPT(&quot;/soap/ajax/37.0/apex.js&quot;)} 

var requestForApproval = new sforce.SObject(&quot;CC_Request_For_Approval__c&quot;);
requestForApproval.id =&quot;{!CC_Request_For_Approval__c.Id}&quot;;

if(&quot;{!CC_Request_For_Approval__c.Distributor_Comments__c}&quot; == &quot;&quot;){
           alert(&quot;Please fill in the reason for Rejection in Distributor Comments&quot;);
}
else{
    requestForApproval.Request_Status__c= &apos;Rejected&apos;;
    sforce.connection.update([requestForApproval]); 
    window.location.reload();
}</url>
    </webLinks>
