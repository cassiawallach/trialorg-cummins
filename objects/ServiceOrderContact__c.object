        <url>{!REQUIRESCRIPT(&apos;/soap/ajax/35.0/connection.js&apos;)}
var recordsSelected = {!GetRecordIds($ObjectType.ServiceOrderContact__c)};
var recordstoupdate = [];
for(var i=0; i &lt; recordsSelected.length ; i++) {
  alert(&apos;Selected ID &apos;+recordsSelected[i]);
  var ServiceOrderContact  = new sforce.SObject(&quot;ServiceOrderContact__c&quot;);
  ServiceOrderContact.Id = recordsSelected[i];
  ServiceOrderContact.Test_Opt_In__c  = true;
  alert(&apos;Checkbox selected &apos;+ServiceOrderContact.Test_Opt_In__c);
  recordstoupdate.push(ServiceOrderContact);
}

var saveResult = sforce.connection.update(recordstoupdate);
alert(&apos;Save Result &apos;+saveResult);
location.reload(true);</url>
