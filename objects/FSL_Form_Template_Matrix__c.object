    <validationRules>
        <fullName>Any_one_should_be_there</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
 ISPICKVAL( Application__c,&quot;&quot;), 
 ISBLANK( Asset__c ), 
 ISBLANK( Engine_Family_Code__c),
 ISBLANK( Repair_Site_Number__c ),
 ISBLANK( Make_Model__c ),
 ISBLANK( Service_Territory__c ),
 ISBLANK( SMN__c ),
 ISBLANK( FSL_Sub_Type__c ),
 ISBLANK( FSL_Work_Type__c ),
 ISBLANK(  Account__c  )
)</errorConditionFormula>
        <errorMessage>Please populate at least one of the Criteria fields</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Choose_One_of_the_Form_Counts</fullName>
        <active>true</active>
        <errorConditionFormula>OR(
AND( One_Form_Dispatched_ServiceAppointment__c , One_Form_Per_Service_Order__c ),
AND(NOT( One_Form_Dispatched_ServiceAppointment__c ),NOT( One_Form_Per_Service_Order__c ))
)</errorConditionFormula>
        <errorMessage>Please select any one &amp; only one of the checkboxs from &quot;One Form Per Service Order&quot; OR &quot;One Form / Dispatched ServiceAppointment&quot;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Operator_should_not_be_blank</fullName>
        <active>true</active>
        <errorConditionFormula>ISPICKVAL( FSL_Query_Operator__c , &quot;&quot;)</errorConditionFormula>
        <errorMessage>Please select the operator</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Prevent_Matrix_activation_based_on_Form</fullName>
        <active>true</active>
        <description>Prevent Update on Matrix if Form Status is &apos;In Use&apos; and Approval Status is &apos;Approved&apos;</description>
        <errorConditionFormula>AND(ISCHANGED(Active__c),
PRIORVALUE(Active__c)= False,
Active__c= True,
OR(
NOT(ISPICKVAL(FSL_Form_Template__r.FSL_Approval_Status__c,&quot;Approved&quot;)),
NOT(ISPICKVAL(FSL_Form_Template__r.disco__Status__c,&quot;In Use&quot;))
)
)</errorConditionFormula>
        <errorMessage>Form Template is not Active or Approved</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>UniqueFormName</fullName>
        <active>false</active>
        <errorConditionFormula>Name == FSL_Form_Template__r.Name</errorConditionFormula>
        <errorMessage>A Form Template Matrix by the same name already exists please update the name in the current Matrix or update the existing one.</errorMessage>
    </validationRules>
