    <validationRules>
        <fullName>FSL_Address_Check</fullName>
        <active>true</active>
        <errorConditionFormula>ISBLANK(City) &amp;&amp; ISBLANK(Street) &amp;&amp; NOT(ISBLANK(Country)) &amp;&amp; ISBLANK(PostalCode) &amp;&amp; ISBLANK(State)</errorConditionFormula>
        <errorDisplayField>Street</errorDisplayField>
        <errorMessage>Please populate the complete technician address to save the record or ensure the country is set as &quot;None&quot;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>FSL__Limit_STM_End_Date</fullName>
        <active>false</active>
        <errorConditionFormula>AND( NOT(ISBLANK(EffectiveEndDate)),
                                EffectiveEndDate &gt; ADDMONTHS(NOW(),36) )</errorConditionFormula>
        <errorMessage>End Date must be within 3 years from today</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>FSL__Secondary_STM</fullName>
        <active>true</active>
        <errorConditionFormula>(ISPICKVAL(TerritoryType, &quot;Secondary&quot;) &amp;&amp; 
    ( ( ( ISCHANGED(Longitude) || ISCHANGED(Latitude) ) &amp;&amp; ( (NOT(ISBLANK(Longitude))) || (NOT(ISBLANK(Latitude))) )) || 
      (NOT(ISBLANK(FSL__Internal_SLR_HomeAddress_Geolocation__Latitude__s))) || 
      (NOT(ISBLANK(FSL__Internal_SLR_HomeAddress_Geolocation__Longitude__s))) || 
      (NOT(ISBLANK( Street ))) || 
      (NOT(ISBLANK( City ))) ||
      (NOT(ISBLANK( Country ))) ||
      (NOT(ISBLANK( State ))) || 
      (NOT(ISBLANK(  PostalCode  )))
    )
 )</errorConditionFormula>
        <errorMessage>Cannot save Service Territory Member of type secondary with Operating Hours or Address</errorMessage>
    </validationRules>
</CustomObject>
