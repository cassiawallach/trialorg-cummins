    <validationRules>
        <fullName>Coupon_Type_Check</fullName>
        <active>true</active>
        <errorConditionFormula>Text( ccrz__CouponType__c )==Null &amp;&amp; INCLUDES( ccrz__Storefront__c ,&apos;SoftwareStore&apos;)</errorConditionFormula>
        <errorMessage>Please select Coupon Type</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>DN_Value_Check</fullName>
        <active>true</active>
        <errorConditionFormula>ISBLANK( DN_Discount__c ) &amp;&amp; INCLUDES(ccrz__Storefront__c ,&apos;SoftwareStore&apos;)</errorConditionFormula>
        <errorMessage>Please Enter 0 in DN price discount field &quot;DN Discount&quot;,Do not leave blank.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>DN_value_check_for_MSRP_coupon</fullName>
        <active>true</active>
        <errorConditionFormula>Text(OSM_Coupon_Discount_Target_Type__c) ==&apos;MSRP&apos; &amp;&amp;  DN_Discount__c &gt;0 &amp;&amp; INCLUDES(ccrz__Storefront__c ,&apos;SoftwareStore&apos;)</errorConditionFormula>
        <errorMessage>You can not set DN Discount value for MSRP coupon</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Discount_Type_check</fullName>
        <active>false</active>
        <errorConditionFormula>ISBLANK(ccrz__DiscountType__c) &amp;&amp; INCLUDES(ccrz__Storefront__c ,&apos;SoftwareStore&apos;)</errorConditionFormula>
        <errorMessage>You can not leave Discount Type field blank,Either choose Absolute or Percentage as per value selected in Coupon Type</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>MSRP_Value_Check</fullName>
        <active>true</active>
        <errorConditionFormula>ISBLANK( ccrz__DiscountAmount__c) &amp;&amp; INCLUDES(ccrz__Storefront__c ,&apos;SoftwareStore&apos;)</errorConditionFormula>
        <errorMessage>Please enter a value or “0” in field “ Discount Amount” . Do not leave blank</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>MSRP_value_check_for_DN_coupon</fullName>
        <active>true</active>
        <errorConditionFormula>(TEXT( OSM_Coupon_Discount_Target_Type__c)==&apos;DN&apos; ) &amp;&amp;  ccrz__DiscountAmount__c &gt;0 &amp;&amp; INCLUDES(ccrz__Storefront__c ,&apos;SoftwareStore&apos;)</errorConditionFormula>
        <errorMessage>You can not set MSRP Discount value for DN coupon</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Rule_Type_Check</fullName>
        <active>true</active>
        <errorConditionFormula>Text(ccrz__RuleType__c)==Null &amp;&amp; INCLUDES(ccrz__Storefront__c ,&apos;SoftwareStore&apos;)</errorConditionFormula>
        <errorMessage>Please select Rule Type General</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Rule_Type_Check_Cart_Total</fullName>
        <active>true</active>
        <errorConditionFormula>TEXT( ccrz__RuleType__c ) ==&apos;CartTotal&apos; &amp;&amp; INCLUDES(ccrz__Storefront__c ,&apos;SoftwareStore&apos;)</errorConditionFormula>
        <errorMessage>You can not select Cart Total in Rule Type</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Rule_Type_Check_Shipping</fullName>
        <active>true</active>
        <errorConditionFormula>Text( ccrz__RuleType__c )==&apos;Shipping&apos; &amp;&amp; INCLUDES(ccrz__Storefront__c ,&apos;SoftwareStore&apos;)</errorConditionFormula>
        <errorMessage>You can not select Shipping in Rule Type</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Storefront_Check</fullName>
        <active>true</active>
        <errorConditionFormula>ISBLANK(ccrz__Storefront__c)|| ISNULL(ccrz__Storefront__c)</errorConditionFormula>
        <errorMessage>Please select Storefront value</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Target_Product_Check</fullName>
        <active>true</active>
        <errorConditionFormula>ISBlank(ccrz__TargetProduct__c) &amp;&amp; ISPICKVAL( ccrz__RuleType__c , &apos;General&apos;) &amp;&amp; INCLUDES(ccrz__Storefront__c ,&apos;SoftwareStore&apos;)</errorConditionFormula>
        <errorMessage>Please select Product in target product field.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Target_Quantity_Check</fullName>
        <active>true</active>
        <errorConditionFormula>ISBlank( Text(ccrz__TargetQty__c)) &amp;&amp; ISPICKVAL( ccrz__RuleType__c , &apos;General&apos;) &amp;&amp; INCLUDES(ccrz__Storefront__c ,&apos;SoftwareStore&apos;)</errorConditionFormula>
        <errorMessage>Please enter value in Target Quantity and also select product record in Target Product field if it is blank</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Target_Relation_Check</fullName>
        <active>true</active>
        <errorConditionFormula>ISBlank( Text( ccrz__TargetRelation__c  )) &amp;&amp;  ISPICKVAL( ccrz__RuleType__c , &apos;General&apos;) &amp;&amp; INCLUDES(ccrz__Storefront__c ,&apos;SoftwareStore&apos;)</errorConditionFormula>
        <errorMessage>Please select value &quot;Product&quot;  in Target Relation and also select product record in Target Product field if it is blank</errorMessage>
    </validationRules>
