                        {{#if this.mediaWrappers.[SVG Interactive Diagram]}}
                            {{#each this.mediaWrappers.[SVG Interactive Diagram]}}
                                <div class="interactive cc_interactive">{{displaySVG this 'mainProdImage prodDetail img-responsive'}}</div>
                            {{/each}}
                        {{/if}}
