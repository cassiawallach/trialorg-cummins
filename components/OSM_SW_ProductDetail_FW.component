                                    {{#each this.mediaWrappers.[Product Image]}}
                                        <div class="col-xs-3">
                                            <img class="alternate cc_alternate img-responsive thumbnail" src="{{productImage this}}" data-id="{{this.uri}}"/>
                                        </div>
                                    {{/each}}
                                    {{#each this.mediaWrappers.[Alternate Images]}}
                                        <div class="col-xs-3">
                                            <img class="alternate cc_alternate img-responsive thumbnail" src="{{productImage this}}" data-id="{{this.uri}}"/>
                                        </div>
                                    {{/each}}
                                </div>
