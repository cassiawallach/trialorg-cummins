                                    <div class="cc_main_prod_image"  style=" width: 65%; margin-left: 65px;">{{displayImage this.product.mediaWrapper 'mainProdImage prodDetail img-responsive'}}</div>
                                </div>
                            </div>
                            {{#if this.mediaWrappers.[Alternate Images]}}
