    <description>This permission set is to give Modify all on Service Job object and delete permission on CSS_User_Location object</description>
    <hasActivationRequired>false</hasActivationRequired>
    <label>CSS_Service_Job_ModifyAll</label>
    <license>Salesforce</license>
    <objectPermissions>
        <allowCreate>false</allowCreate>
        <allowDelete>true</allowDelete>
        <allowEdit>true</allowEdit>
        <allowRead>true</allowRead>
        <modifyAllRecords>true</modifyAllRecords>
        <object>CSS_Job_Order__c</object>
        <viewAllRecords>true</viewAllRecords>
    </objectPermissions>
    <objectPermissions>
        <allowCreate>false</allowCreate>
        <allowDelete>true</allowDelete>
        <allowEdit>true</allowEdit>
        <allowRead>true</allowRead>
        <modifyAllRecords>false</modifyAllRecords>
        <object>CSS_UserLocation__c</object>
        <viewAllRecords>false</viewAllRecords>
    </objectPermissions>
