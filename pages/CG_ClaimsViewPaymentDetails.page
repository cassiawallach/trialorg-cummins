                                            <th class="tblhead" style="text-align:center;width:70px;"><b>{!$Label.CG_Calibration_Before}</b></th>
                                            <th class="tblhead" style="text-align:center;"><b>{!$Label.CG_Revision_Before}</b> </th>
                                            <th class="tblhead" style="text-align:center;width:70px;"><b>{!$Label.CG_Calibration_After}</b></th>
                                            <th class="tblhead" style="text-align:center;"><b>{!$Label.CG_Revision_After}</b></th>
                                        </tr>
