                                                <td class="pt-3-half" style="text-align:center;"><apex:outputText style="padding:0px" value="{!claimcomp.revisionAfter}" /></td>            

                                            </tr>
                                        </apex:repeat>
                                    </tbody>
                                </table>
                            </div>    
                        </div>    
                    </div>
