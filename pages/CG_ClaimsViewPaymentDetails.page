                                                <td class="pt-3-half" style="text-align:center;"><apex:outputText value="{!claimcomp.WSD}"></apex:outputText></td>
                                                <td class="pt-3-half" style="text-align:center;"><apex:inputCheckbox disabled="true" selected="{!claimcomp.BIS}"/> </td>
                                                <td class="pt-3-half" style="text-align:center"><apex:outputText value="{!claimcomp.VIN}" /></td>
                                                <td class="pt-3-half" style="text-align:center;"><apex:outputText style="padding:0px" value="{!claimcomp.calibrationBefore}"/></td>
                                                <td class="pt-3-half" style="text-align:center;"><apex:outputText style="padding:0px" value="{!claimcomp.revisionBefore}" /></td>
