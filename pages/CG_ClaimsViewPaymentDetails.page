        table.ViewPaymentDetails tr:nth-child(odd) {
            background: #e2e2e2;
        }

        table.ViewPaymentDetails tr:nth-child(even) {
            background: #f9f9f9;
        }


        table.cmi_jobplan-table tr:nth-child(even) {
            background: #f9f9f9;
        }

        table.cmi_jobplan-table tr:nth-child(odd) {
            background: #e2e2e2;
        }
        table.cmi_jobplan-table thead tr{
            background: #ccc;
