            } else {
                addErrorSpan($('[id*="email"]'), $('[id*="email"]')[0].dataset['fnameis']);
                return false;
            }*/
        }
    }
    
    function validateEmail(mail) {
        if (/^[a-zA-Z0-9.!#$%&'*+/=?^_`{|}~-]+@[a-zA-Z0-9](?:[a-zA-Z0-9-]{0,61}[a-zA-Z0-9])?(?:\.[a-zA-Z0-9](?:[a-zA-Z0-9-]{0,61}[a-zA-Z0-9])?)*$/.test(mail)) {
            return true;
        } else {
            return false;
        }
    }
    

