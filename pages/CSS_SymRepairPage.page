        function checkRadioButtons(val){
            if(val == '') return;
            var icons = $('#rbtnIcons >li > input');
            $.each(icons,function(idx,item){
                item.checked = false;
                if($(this).data('icon') == val ){
                    item.checked = true;
                }
            });
        }


        function getIcon(num){
            var icon = "";
            num = num.toString();
            switch(num) {
                case '1':
                    icon = 'icon-correct';
                    break;
                case '2':
                    icon = 'icon-correctplus';
                    break;
                case '3':
                    icon = 'icon-wrong'
                    break;
                case '4':
                    icon = 'icon-qtn';
                    break;
            }
            return icon;
        } 

