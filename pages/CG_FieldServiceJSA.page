                                    {!$Label.CG_SeriousInjuryHA}
                                </td>
                            </tr>
                            <tr>
                                <td colspan="2" style="padding:10px;"><span style="color:red"> <b> {!$ObjectType.CSS_JobForms__c.fields.MOVING_VEHICLES__c.Label} </b> </span></td>
                                <td colspan="2" align="center"><span style="color:red"> <b> {!$ObjectType.CSS_JobForms__c.fields.MOVING_MACHINERY__c.Label} </b> </span></td>
                                <td colspan="2" align="center"><span style="color:red"> <b> {!$ObjectType.CSS_JobForms__c.fields.WORK_AT_HEIGHT__c.Label} </b> </span></td>
                            </tr>
                            <tr>
                                <td colspan="2" style="padding:10px;border-top:0px;"><span style="color:red"> <b> {!$ObjectType.CSS_JobForms__c.fields.WORK_WITH_ELECTRICITY__c.Label} </b> </span></td>
                                <td colspan="2" align="center" style="border-top:0px;"><span style="color:red"> <b> {!$ObjectType.CSS_JobForms__c.fields.LIFTING_OPERATOIONS__c.Label} </b></span></td>
