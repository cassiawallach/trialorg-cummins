<apex:page showHeader="false" standardStylesheets="false" controller="CG_CL_FieldServiceJSA">
    <head>
        <apex:stylesheet value="{!URLFOR($Resource.CSS_Standard_UI, '/Cummins_New1_UI/Cummins_New1_UI/assets/css/component_specific/jquery.dataTables.css')}"/>      
        <link rel="stylesheet" href="{!URLFOR($Resource.CSS_Standard_UI, '/Cummins_New1_UI/Cummins_New1_UI/assets/bootstrap3/css/CMI-bootstrap.css')}"/>
        <SCRIPT type="text/javascript" src="{!URLFOR($Resource.CSS_Standard_UI, '/Cummins_New1_UI/Cummins_New1_UI/assets/js/jquery-1.11.2.min.js')}"/>
        <SCRIPT type="text/javascript" src="{!URLFOR($Resource.CSS_Standard_UI, '/Cummins_New1_UI/Cummins_New1_UI/assets/bootstrap3/js/bootstrap.js')}"/>
        <style>

            .cmi_cummins_logo {
                background: url("{!URLFOR($Resource.CSS_Standard_UI, '/Cummins_New1_UI/Cummins_New1_UI/assets/img/Cummins_logo.png')}") no-repeat 0 0;
                width: 152px;
                height: 133px;
                zoom:0.5;
            }

            .cmi_QSOL_logo {
                background: url("{!URLFOR($Resource.CSS_Standard_UI, '/Cummins_New1_UI/Cummins_New1_UI/assets/img/QSOL.png')}") no-repeat 0 0;
                width: 200px;
                height: 80px;
                display:inline-block;
            } 

            .logo {
                display: block;
                padding-left: 0px;
                padding-right: 0px;
            }

            .logo_img {
                display: inline-block;
                padding: 0px 0px 0px 30px;
                float: left;
                width: 117px;
                height: 74px;
            }

            .sub-brand {
                padding: 0px;
                display: inline-block;
            }

            table.cmi_inShopJSA .readOnly {
                background-color:#d2d2d2;
            }

            table.cmi_inShopJSA tr td {
                border:2px solid #000;
            } 

            table.cmi_inShopJSA tr td:nth-of-type(1) {
                width:10%;
                border-bottom:0px;
            }

            table.cmi_inShopJSA tr td {
                width:20%;
            }

            table.cmi_inShopJSA_Prepare tr td, table.cmi_TELA{
                vertical-align:middle;
            }

            .prepareRadiobuttons {
                width:200px;
            }

            table.cmi_inShopJSA_Hazard tr td {
                border:2px solid #000;
                vertical-align:middle;
            }

            .InShopCheckbox{
                float:left;
                width:35px;
                height:20px;
           }

            table.cmi_InShop_HIdentification tr td{
                border : 1px solid #000;
            }

            #ReportInjury:after {
                content: "";
                background-color: #d2d2d2;
                position: absolute;
                width: 2px;
                height: 565px;
                top: 0px;
                left: 52%;
                display: block;
            }

            .saveButton {
                display: inline-block;
                position: relative;
                color: #fff !important;
                border: none;
                border-radius: 5px;
                padding: 5px 20px;
                font-size: 14px;
                cursor: pointer;
                background: linear-gradient(#006666, #006666 37%, #003536 57%, #003536);
                filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#007C7C', endColorstr='#007C7C',GradientType=0 ); /* IE6-9 */
            }

            .cancelButton {
                display: inline-block;
                position: relative;
                color: #FFF;
                border: none;
                border-radius: 5px;
                padding: 5px 20px;
                font-size: 14px;
                cursor: pointer;
                background: #333333; /* Old browsers */
                background: -moz-linear-gradient(top,  #333333 0%, #333333 100%); /* FF3.6+ */
                background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#333333), color-stop(100%,#333333)); /* Chrome,Safari4+ */
                background: -webkit-linear-gradient(top,  #333333 0%,#333333 100%); /* Chrome10+,Safari5.1+ */
                background: -o-linear-gradient(top,  #333333 0%,#333333 100%); /* Opera 11.10+ */
                background: -ms-linear-gradient(top,  #333333 0%,#333333 100%); /* IE10+ */
                background: linear-gradient(to bottom,  #333333 0%,#333333 100%); /* W3C */
                filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#333333', endColorstr='333333',GradientType=0 ); /* IE6-9 */
            }

            .cmi_inShopJSA_Prepare > tbody > tr > td ,  .cmi_TELA > tbody > tr > td {
                padding:0px 10px;
                vertical-align:middle;
            }

            table.cmi_inShopJSA_Prepare label ,  table.cmi_TELA label {
                font-weight:normal;
            }

            a.cancelButton:hover {
                color:#fff;
                text-decoration:none;
            }

            .responsive {
                width: 100%;
                height: auto;
            }


        </style>
    </head>

    <body>

        <script>
            function DisableButton(saveBtn){
                saveBtn.disabled=true;
                FieldServicePgSaveJS();
            }

            function DisableButtonSaveAndClose(saveBtn){
                saveBtn.disabled=true;
            }

            function onCompleteSaveAndClose() {
                // window.opener.top.location.reload();
                window.close();
            }
        </script>

        <apex:form >   
            <apex:outputPanel rendered="{!IF(copy == 'false',true,false)}">
                <div class="panel-body" style="border:2px solid #000;padding:0px;margin:10px;">

                    <div class="col-sm-12" style="padding:0px;margin:10px 0px;">

                        <div class="col-sm-9" style="border:1px solid #000">
                            <div class="logo col-sm-7">
                                <div class="logo_img" style="margin:10px 0px 0px 0px">
                                    <div class="cmi_cummins_logo"></div>
                                </div>
                                <div class="sub-brand">
                                    <h3 style="color:red;margin-top:5px;"><b>Distribution</b></h3>
                                </div>
                            </div>

                            <div class="col-sm-5" style="height:90px;padding:5px">
                                <h2><b>{!(UPPER($Label.CG_FieldServiceJSA))}</b> </h2>
                            </div>
                            <div class="clearfix"></div>
                        </div>
                        
                        <div class="col-sm-3" style="border:1px solid #000;">
                              <div class="col-sm-12" style="margin:20px 10px">{!$Label.CG_Date} : {!currentDateTime} </div>   
                            <div class="col-sm-12" style="margin:0px 10px 10px 10px"> {!$Label.CG_WorkOrder} # {!workJob.name}</div>
                        </div>
                    </div> 
                    <div class="clearfix"></div>
                    
                   <table class="table table-bordered cmi_inShopJSA" style="margin-bottom:0px;">
                            <tr class="readOnly">
                                <td style="padding:8px 30px;"> {!$Label.CG_EmployeeName}</td>
                                <td> {!UserName}</td>
                                <td style="border:3px solid #000;" align="center"> {!$Label.CG_WWID}</td> 
                                <td>{!WWID}</td>
                            </tr>

                            <tr>
                                <td class="readOnly" style="border-top:0px;padding:8px 30px;"> {!$Label.CG_JSACompletedwith} </td>
                                <td class="readOnly" style="padding:15px;border:3px solid #000;">{!$ObjectType.CSS_JobForms__c.fields.Co_Worker_Name__c.Label}:</td>
                                <td colspan="2" style="padding:0px"> 
                                    <apex:inputTextarea style="width:100%;resize:none;height:50px;border:0px" value="{!FieldServiceForm.Co_Worker_Name__c}" html-maxlength="255"/> 
                                </td>
                            </tr>
                            <tr>
                                <td class="readOnly" style="border-top:0px;padding:8px 30px;">{!$ObjectType.CSS_JobForms__c.fields.Task_Category__c.Label}</td>
                                <td style="padding:0px"> 
                                    <apex:selectList size="1" style="width:100%;height:35px;border:0px;" value="{!FieldServiceForm.Task_Category__c}">
                                        <apex:selectOptions value="{!TaskCategory}"/> 
                                    </apex:selectList>
                                </td>
                                <td colspan="2" class="readOnly"></td>
                            </tr>

                            <tr>
                                <td class="readOnly" style="border-top:0px;padding:8px 30px;"> {!$ObjectType.CSS_JobForms__c.fields.Task_Description__c.Label}</td>
                                <td colspan="3" style="padding:0px">
                                    <apex:inputTextarea style="width:100%;resize:none;height:50px;border:0px;" value="{!FieldServiceForm.Task_Description__c}" html-maxlength="255"/>
                                </td>
                            </tr>

                            <tr>
                                <td class="readOnly" style="border-top:0px;border-bottom:2px solid #000;padding:8px 30px;"> {!$ObjectType.CSS_JobForms__c.fields.Job_Location__c.Label}</td>
                                <td colspan="3" style="padding:0px">
                                    <apex:inputTextarea style="width:100%;resize:none;height:50px;" value="{!FieldServiceForm.Job_Location__c}" html-maxlength="255"/>
                                </td>
                            </tr>
                        </table>
                  

                    <!-- PREPARE Table -->
                    <div style="padding:0px 15px">
                        <table class="table cmi_inShopJSA_Prepare" style="border:2px solid #000;margin-bottom:10px">
                            <tr bgcolor ="#d2d2d2" style="border-bottom:2px solid #000">
