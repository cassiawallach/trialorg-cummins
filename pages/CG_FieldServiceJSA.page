                                    <apex:selectRadio styleClass="prepareRadiobuttons" value="{!FieldServiceForm.LiftingAccess2__c}">
                                        <apex:selectOptions value="{!ItemsPrepare}" />
                                    </apex:selectRadio>
                                </td>
                            </tr>
                            <tr>
                                <td style="border-top:0px">{!$Label.CG_LiftingAccess3}</td>
                                <td style="border-top:0px">
                                    <apex:selectRadio styleClass="prepareRadiobuttons" value="{!FieldServiceForm.LiftingAccess3__c}">
                                        <apex:selectOptions value="{!ItemsPrepare}" />
                                    </apex:selectRadio>
                                </td>
                            </tr>
                        </table>
                    </div>

                    <!--PPE -->

                    <div style="padding:0px 15px">
                        <table class="table cmi_inShopJSA_Prepare" style="border:2px solid #000;margin-bottom:10px;">
                            <tr bgcolor ="#d2d2d2" style="border-bottom:3px solid #000">
                                <td colspan="4" style="padding:8px">{!$Label.CG_PPE} </td>
                            </tr>  
                            <tr>
                                <td class="col-sm-4">{!$ObjectType.CSS_JobForms__c.fields.PPE1__c.Label}</td>
                                <td class="col-sm-2" style="border-right:2px solid #000;">
                                    <apex:selectRadio styleClass="prepareRadiobuttons" value="{!FieldServiceForm.PPE1__c}">
                                        <apex:selectOptions value="{!PPE}" />
                                    </apex:selectRadio>
                                </td>
                                <td class="col-sm-4" style="padding-right: 0px" >{!$Label.CG_PPESuitable}</td>
                                <td class="col-sm-2">
                                    <apex:selectRadio styleClass="prepareRadiobuttons" value="{!FieldServiceForm.PPE2__c}">
                                        <apex:selectOptions value="{!PPE}" />
                                    </apex:selectRadio>
                                </td>
                            </tr>
                        </table> 
                    </div>

                    <hr style="margin:5px 0px;border-top:2px solid #000"/>

                    <!-- Lone Working -->

                    <div style="padding:0px 15px">
                        <table class="table cmi_inShopJSA_Prepare" style="border:2px solid #000;margin-bottom:0px;">
                            <tr style="border-bottom:2px solid #000">
                                <td bgcolor ="#d2d2d2" style="width:150px;border-right:2px solid #000;">{!$ObjectType.CSS_JobForms__c.fields.LoneWorking__c.Label}</td>
                                <td style="border-bottom:2px solid #000;width:35%"> {!$Label.CG_LoneWorking}  </td>
                                <td style="border-right:2px solid #000;">
                                    <apex:selectRadio styleClass="prepareRadiobuttons" value="{!FieldServiceForm.LoneWorking__c}">
                                        <apex:selectOptions value="{!PPE1}" />
                                    </apex:selectRadio>     
                                </td>     
                            </tr>  
                            <tr style="border-top:2px solid #000">
                                <td colspan="4" style="padding:0px"> 
                                    <div>{!$Label.CG_LoneWorkingInputText}:</div>
