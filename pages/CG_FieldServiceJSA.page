                    <!-- Lifting And Access -->

                    <div class="col-sm-6" style="padding-left:0px">
                        <table class="table cmi_TELA" style="border:2px solid #000;margin-bottom:0px">
                            <tr bgcolor ="#d2d2d2" style="border-bottom:2px solid #000">
                                <td>{!$ObjectType.CSS_JobForms__c.fields.LiftingAccess1__c.Label}</td>
