<aura:component description="footer" implements="forceCommunity:availableForAllPageTypes" access="global">
    <aura:attribute name="footer" type="String" default="Cummins" access="global" />
    <aura:attribute name="footerYear" type="String" default="2020" access="global" />
    <div class="grid" style="padding-right: 20px;color: #fff;">
                             
        <div class="container-fluid" style="padding-left:30.5em">
            <img src="{!$Resource.CS_Cumminsgf}" class="global-footer-logo" style="padding-top: 12px;padding-right: 50px; padding-bottom: 12px;padding-left: 2px;"/>
            <!--span class="global-footer-copy">© {!v.footerYear} Cummins Inc., Box 3005, Columbus, IN 47202-3005 U.S.A.</span-->
            <span class="global-footer-copy">© {!v.footerYear} Cummins Inc., Box 3005, Columbus, IN 47202-3005 U.S.A.</span><br></br>
            <div style="text-align:left;padding-left: 100px;font-size: 16px;">
                
                <a style="padding-right: 20px;
                          color: #fff; text-decoration: underline;" href="https://care.cummins.com" target="_blank">{!$Label.c.FSL_CSSP_Cummins_Care}</a>
                <a style="padding-right: 20px;
                          color: #fff; text-decoration: underline;" href="https://www.cummins.com/company/diversity/accessibility" target="_blank">	{!$Label.c.FSL_CSSP_Accessibility}</a>
                <a style="padding-right: 20px;
                          color: #fff; text-decoration: underline;" href="https://www.cummins.com/privacy-and-legal" target="_blank">{!$Label.c.FSL_CSSP_Privacy_Legal}</a>
                
            </div>  
        </div>
        
    </div>
    
